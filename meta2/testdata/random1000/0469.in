char **id(char id)
{
    char **id, *id, id[12];

    while ("str"
	   && ("str"
	       || "str" == itoa(12, !'c')) / atoi(itoa(id,
						       id(12, -"str") + 12)
						  || ("str") + 12 * 'c' <=
						  'c') <=
	   'c' +
	   (12)[id
		((itoa(printf((id())[12]), id())
		  && atoi("str" + !"str")[*id]))] < 12[!id()])
	return +"str";
}
